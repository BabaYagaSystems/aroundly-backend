{
  "realm": "glimpse", 
  "enabled": true,
  "registrationAllowed": true,
  "loginWithEmailAllowed": true,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,
  "verifyEmail": false,
  "registrationEmailAsUsername": false,
  "loginTheme": "keycloak",

  "passwordPolicy": "length(8) and digits(1) and upperCase(1) and specialChars(1) and notUsername()",
  "displayName": "glimpse realm",

  "clientScopeMappings": {
    "realm-management": [
      {
        "client": "aroundly",
        "roles": [ "manage-users", "view-users", "query-users" ]
      }
    ]
  },

  "clients": [
    {
      "clientId": "aroundly",
      "protocol": "openid-connect",
      "publicClient": false,
      "secret": "aroundly-secret",
      "redirectUris": [
        "http://localhost:8100/login/oauth2/code/keycloak"],
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": true,
      "standardFlowEnabled": true
    }
  ]
}
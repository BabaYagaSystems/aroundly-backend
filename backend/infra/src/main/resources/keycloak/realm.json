{
  "realm": "glimpse",
  "enabled": true,
  "registrationAllowed": true,
  "loginWithEmailAllowed": true,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,

  "passwordPolicy": "length(8) and digits(1) and upperCase(1) and specialChars(1) and notUsername()",
  "displayName": "glimpse realm",

  "clients": [
    {
      "clientId": "aroundly",
      "protocol": "openid-connect",
      "publicClient": false,
      "secret": "aroundly-secret",
      "redirectUris": [
        "http://localhost:8100/login/oauth2/code/keycloak"],
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": true,
      "standardFlowEnabled": true
    }
  ],
  "roles": {
    "client": {
      "aroundly": [
        {
          "name": "USER",
          "description": "default user of the application"
        },
        {
          "name": "BUSINESS",
          "description": "the event creator"
        },
        {
          "name": "ADMIN",
          "description": "the messiah, the Jesus Christ of aroundly. God itself!"
        }
      ]
    }
  }
}